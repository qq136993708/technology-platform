<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pcitc.mapper.out.OutPersonVoMapper">
  <resultMap id="BaseResultMap" type="com.pcitc.base.out.OutPersonVo">
    
    <!--人员编号，专家的主键  -->
    <result column="PERNR" jdbcType="VARCHAR" property="pernr" />
    <!--单位编码  -->
    <result column="ZDWBM" jdbcType="VARCHAR" property="zdwbm" />
    <!--单位全称  -->
    <result column="ZDWQC" jdbcType="VARCHAR" property="zdwqc" />
    <!-- 姓名  -->
    <result column="NACHN" jdbcType="VARCHAR" property="nachn" />
    <!-- 性别  -->
    <result column="GESCH" jdbcType="VARCHAR" property="gesch" />
    <!--出生日期  -->
    <result column="GBDAT" jdbcType="VARCHAR" property="gbdat" />
    <!--身份证号码 -->
    <result column="ICNUM" jdbcType="VARCHAR" property="icnum" />
    <!--单位名称  -->
    <result column="ZDWMC" jdbcType="VARCHAR" property="zdwmc" />
    <!--职务级别名称  -->
    <result column="ZZWJBMC" jdbcType="VARCHAR" property="zzwjbmc" />
    <!--职务级别编码  -->
    <result column="ZZWJBBM" jdbcType="VARCHAR" property="zzwjbbm" />
    <!--最高学历   -->
    <result column="ZJYZX" jdbcType="VARCHAR" property="zjyzx" />
     <!--最高学历编码  -->
    <result column="ZJYXLBM_N" jdbcType="VARCHAR" property="zjyxlbmn" />
    <!--聘任专业技术职务级别 编码 -->
    <result column="ZPRZYJSZWJBBM" jdbcType="VARCHAR" property="zprzyjszwjbbm" />
    <!--聘任专业技术职务级别 名称  -->
    <result column="ZPRZYJSZWJBMC" jdbcType="VARCHAR" property="zprzyjszwjbmc" />
    
    
     <!--专业技术资格名称  -->
    <result column="ZZYJSZGMCMC" jdbcType="VARCHAR" property="zzyjszgmcmc" />
     <!--专业技术资格编码  -->
    <result column="ZZYJSZGMCBM" jdbcType="VARCHAR" property="zzyjszgmcbm" />
    
    
    
    
    
    
    <!--获得专利时间 -->
    <result column="ZHDZLSJ"  jdbcType="VARCHAR" property="zhdzlsj" />
    <!--序号 -->
    <result column="SEQNR_9009"    jdbcType="VARCHAR" property="seqnr9009" />
    <!--专利名称-->
    <result column="ZFMZLSJ"  jdbcType="VARCHAR" property="zfmzlsj" />
    <!--专利号 -->
    <result column="ZZLH"     jdbcType="VARCHAR" property="zzlh" />
    <!--专利排名 -->
    <result column="ZZLPM"    jdbcType="VARCHAR" property="zzlpm" />
    
    
    
    
     <!-- 著作 序号 -->
    <result column="SEQNR_9010"    jdbcType="VARCHAR" property="seqnr9010" />
     <!--论文、著作名称 -->
    <result column="ZLZMC"    jdbcType="VARCHAR" property="zlzmc" />
     <!--发表时间 -->
    <result column="ZFBSJ"    jdbcType="VARCHAR" property="zfbsj" />
     <!--著作方式 -->
    <result column="ZZZFS"    jdbcType="VARCHAR" property="zzzfs" />
     <!--刊物或出版社名称 -->
    <result column="ZCBSMC"    jdbcType="VARCHAR" property="zcbsmc" />
     <!--出版国家 -->
    <result column="ZCBGJ"    jdbcType="VARCHAR" property="zcbgj" />
     <!--备注 -->
    <result column="ZLZBZ"    jdbcType="VARCHAR" property="zlzbz" />
    
    
    <!--获奖 序号 -->
    <result column="SEQNR_9007"    jdbcType="VARCHAR" property="seqnr9007" /> 
    <!--高层次人才等级编码 -->
    <result column="ZGCCRCDJBM"    jdbcType="VARCHAR" property="zgccrcdjbm" />
    <!--高层次人才等级名称 -->
    <result column="ZGCCRCDJMC"    jdbcType="VARCHAR" property="zgccrcdjmc" />
    <!--高层次人才类别编码 -->
    <result column="ZGCCRCLB1BM"    jdbcType="VARCHAR" property="zgccrclb1bm" />
    <!--高层次人才类别名称 -->
    <result column="ZGCCRCLB1MC"    jdbcType="VARCHAR" property="zgccrclb1mc" />
    <!--获奖类别编码 -->
    <result column="ZHJLB"    jdbcType="VARCHAR" property="zhjlb" />
    <!--获奖类别名称 -->
    <result column="ZHJLBMS"    jdbcType="VARCHAR" property="zhjlbms" />
    <!--获奖级别编码 -->
    <result column="ZHJJB"    jdbcType="VARCHAR" property="zhjjb" />
    <!--获奖级别名称 -->
    <result column="ZHJJBMS"    jdbcType="VARCHAR" property="zhjjbms" />
    <!--授予时间 -->
    <result column="ZSYSJ"    jdbcType="VARCHAR" property="zsysj" />
    
    
    
    
    <!--获奖 序号 -->
    <result column="SEQNR_9025" jdbcType="VARCHAR" property="seqnr9025" />
    <!--奖励或惩罚名称 -->
    <result column="ZJCMC"    jdbcType="VARCHAR" property="zjcmc" />
    <!--奖励种类 -->
    <result column="ZJLZL"    jdbcType="VARCHAR" property="zjlzl" />
    <result column="ZJLZLBM"    jdbcType="VARCHAR" property="zjlzlbm" />
    <!--奖惩类别 -->
    <result column="ZJCLB"    jdbcType="VARCHAR" property="zjclb" /> 
    <result column="ZJCLBBM"    jdbcType="VARCHAR" property="zjclbbm" /> 
    <!--奖惩情况描述 -->
    <result column="ZJCQKMS"    jdbcType="VARCHAR" property="zjcqkms" /> 
    <!--奖惩级别 -->
    <result column="ZJCJB"    jdbcType="VARCHAR" property="zjcjb" /> 
    <result column="ZJCJBBM"    jdbcType="VARCHAR" property="zjcjbbm" /> 
    <!--批准时间 -->
    <result column="ZPZSJ"    jdbcType="VARCHAR" property="zpzsj" /> 
    <!--奖惩批准单位 -->
    <result column="ZJCPBDW"    jdbcType="VARCHAR" property="zjcpbdw" /> 
    
    
  </resultMap>
  
  <!--   获取所有专家-->
  <select id="getHanaOutPersonVoList" parameterType="java.util.Map" resultMap="BaseResultMap">
        select 
         PERNR,
         ZDWBM,
         ZDWMC ,
         NACHN,
         GESCH,
         GBDAT,
         ICNUM,
         ZZWJBMC,
         ZZWJBBM,
         ZJYZX,
         ZJYXLBM_N,
         ZPRZYJSZWJBBM,
         ZPRZYJSZWJBMC,ZZYJSZGMCMC,ZZYJSZGMCBM
         from "_SYS_BIC"."ZHJT.HR/CAL_HR_KJRC"
  </select>
  <!-- 获取所有专家相关专利 -->
   <select id="getHanaOutPersonPatentList" parameterType="java.util.Map" resultMap="BaseResultMap">
         select 
         PERNR,ZHDZLSJ,SEQNR_9009,ZFMZLSJ,ZZLH,ZZLPM
         from "_SYS_BIC"."ZHJT.HR/CAL_HR_KJRC"
  </select>
  <!-- 获取所有专家 相关   论著 -->
   <select id="getHanaOutPersonBookList" parameterType="java.util.Map" resultMap="BaseResultMap">
         select 
         PERNR,SEQNR_9010,ZLZMC,ZFBSJ,ZZZFS,ZCBSMC,ZLZBZ
         from "_SYS_BIC"."ZHJT.HR/CAL_HR_KJRC"
  </select>
  
  <!-- 获取获奖  -->
   <select id="getHanaOutPersonAwardList" parameterType="java.util.Map" resultMap="BaseResultMap">
         select 
         PERNR,SEQNR_9007,ZGCCRCDJBM,ZGCCRCDJMC,
         ZGCCRCLB1BM,
         ZGCCRCLB1MC,
         ZHJLB,
         ZHJLBMS,
         ZHJJB,
         ZHJJBMS,
         ZSYSJ
         from "_SYS_BIC"."ZHJT.HR/CAL_HR_KJRC"
  </select>
  
  
  
  
  
  
  <!-- 奖惩  -->
   <select id="getHanaOutPersonPunishList" parameterType="java.util.Map" resultMap="BaseResultMap">
      select 
         PERNR,
         SEQNR_9025,
         ZJCMC,
         ZJLZL,
         ZJLZLBM,
         ZJCLB,
         ZJCLBBM,
         ZJCQKMS,
         ZJCJB,
         ZJCJBBM,
         ZPZSJ,
         ZJCPBDW
       from "_SYS_BIC"."ZHJT.HR/CAL_HR_KJRC"
  </select>
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  <select id="getHanaPantentListByNum" parameterType="string" resultMap="BaseResultMap">
        select ZHDZLSJ,SEQNR_9009,ZFMZLSJ,ZZLH,ZZLPM  
           from "_SYS_BIC"."ZHJT.HR/CAL_HR_KJRC" 
        where  PERNR=#{pernr}
  </select>
  
  
</mapper>