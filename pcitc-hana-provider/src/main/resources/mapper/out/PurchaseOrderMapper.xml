'PLACEHOLDER' = ('$$PR_CALMONTH$$','${month}'),
'PLACEHOLDER' = ('$$PR_GSDM$$','1020,1040,1041,1060,1061,1080,1100,1101,1120,1123,1124,1127,1130,4360')
)
