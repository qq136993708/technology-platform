	 "G0MATNR",
	 "G0MAKTX",
	 "G0EBELN",
	 "G0EBELP",
	 "G0TXZ01",
	 "G0EINDT",
	 "G0LIFNR",
	 "G0NAME2",
	 "G0DDLX",
	 sum("K0XMDDSL") AS "K0XMDDSL",
	 sum("K0XMDDJE") AS "K0XMDDJE"
FROM "_SYS_BIC"."SINOPEC.ZBYY.KJBYY.001_KYGLFXYY/H1AM_KY_YW_1000_02"
