<mapper namespace="com.pcitc.mapper.computersoftware.ComputerSoftwareMapper">

    <resultMap id="BaseResultMap" type="com.pcitc.base.computersoftware.ComputerSoftware">

        <id column="ID" jdbcType="VARCHAR" property="id"/>
        <result column="UNIT_NAME" jdbcType="VARCHAR" property="unitName"/>
        <result column="REGISTER_NUMBER" jdbcType="VARCHAR" property="registerNumber"/>
        <result column="SOFTWARE_NAME" jdbcType="VARCHAR" property="softwareName"/>
        <result column="COPYRIGHT_OWNER" jdbcType="VARCHAR" property="copyrightOwner"/>
        <result column="VERSION_NUMBER" jdbcType="VARCHAR" property="versionNumber"/>
        <result column="RECORD_DATE" jdbcType="TIMESTAMP" property="recordDate"/>
        <result column="DEVELOP_FINISH_DATE" jdbcType="TIMESTAMP" property="developFinishDate"/>
        <result column="SOFTWARE_INTRO" jdbcType="VARCHAR" property="softwareIntro"/>
        <result column="ENTRY_PEOPLE" jdbcType="VARCHAR" property="entryPeople"/>
        <result column="ENTRY_TIME" jdbcType="TIMESTAMP" property="entryTime"/>
        <result column="NOTES" jdbcType="VARCHAR" property="notes"/>
        <result column="ACCESSORY_UPLOAD" jdbcType="VARCHAR" property="accessoryUpload"/>
    </resultMap>

    <sql id="Base_Column_List">
      ID,
      UNIT_NAME,
      REGISTER_NUMBER,
      SOFTWARE_NAME,
      COPYRIGHT_OWNER,
      VERSION_NUMBER,
      RECORD_DATE,
      DEVELOP_FINISH_DATE,
      SOFTWARE_INTRO,
      ENTRY_PEOPLE,
      ENTRY_TIME,
      NOTES,
      ACCESSORY_UPLOAD
  </sql>

    <select id="load" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from computer_software
        where id = #{id} and deleted != '1'
    </select>


    <update id="delete" parameterType="java.lang.String">
            update computer_software set deleted = '1' where id = #{id}
    </update>


    <insert id="add" parameterType="com.pcitc.base.computersoftware.ComputerSoftware">
    insert into computer_software (
      UNIT_NAME,
      REGISTER_NUMBER,
      SOFTWARE_NAME,
      COPYRIGHT_OWNER,
      VERSION_NUMBER,
      RECORD_DATE,
      DEVELOP_FINISH_DATE,
      SOFTWARE_INTRO,
      ENTRY_PEOPLE,
      ENTRY_TIME,
      NOTES,
      ACCESSORY_UPLOAD
      )
    values (
       #{unitName},
       #{registerNumber},
       #{softwareName},
       #{copyrightOwner},
       #{versionNumber},
       #{recordDate},
       #{developFinishDate},
       #{softwareIntro},
       #{entryPeople},
       #{entryTime},
       #{notes},
       #{accessoryUpload}
      )
  </insert>



    <update id="update" parameterType="com.pcitc.base.computersoftware.ComputerSoftware">
        update computer_software
        <trim prefix="SET" suffixOverrides=",">
            <if test="unitName != null and unitName != '' ">
                UNIT_NAME = #{UNIT_NAME},
            </if>
            <if test="registerNumber != null and registerNumber != '' ">
                REGISTER_NUMBER = #{REGISTER_NUMBER},
            </if>
            <if test="softwareName != null and softwareName != ''">
                SOFTWARE_NAME = #{SOFTWARE_NAME},
            </if>
            <if test="copyrightOwner != null and copyrightOwner != ''">
                COPYRIGHT_OWNER = #{COPYRIGHT_OWNER},
            </if>
            <if test="versionNumber != null and versionNumber != ''">
                VERSION_NUMBER = #{VERSION_NUMBER},
            </if>
            <if test="recordDate != null and recordDate != ''">
                RECORD_DATE = #{RECORD_DATE},
            </if>
            <if test="developFinishDate != null and developFinishDate != ''">
                DEVELOP_FINISH_DATE = #{DEVELOP_FINISH_DATE},
            </if>
            <if test="softwareIntro != null and softwareIntro != ''">
                SOFTWARE_INTRO = #{SOFTWARE_INTRO},
            </if>
            <if test="entryPeople != null and entryPeople != ''">
                ENTRY_PEOPLE = #{ENTRY_PEOPLE},
            </if>
            <if test="entryTime != null and entryTime != ''">
                ENTRY_TIME = #{ENTRY_TIME},
            </if>
            <if test="notes != null and notes != ''">
                NOTES = #{NOTES},
            </if>
            <if test="accessoryUpload != null and accessoryUpload != ''">
                ACCESSORY_UPLOAD = #{ACCESSORY_UPLOAD}
            </if>
        </trim>
        where id = #{id}
    </update>



    <select id="query" parameterType="java.util.Map" resultMap="BaseResultMap">
        select * from computer_software t where 1=1

        <if test="unitName != null and unitName != '' ">
            UNIT_NAME = #{UNIT_NAME}
        </if>
        <if test="registerNumber != null and registerNumber != ''">
            REGISTER_NUMBER = #{REGISTER_NUMBER}
        </if>
        <if test="softwareName != null and softwareName != ''">
            SOFTWARE_NAME = #{SOFTWARE_NAME}
        </if>
        <if test="copyrightOwner != null and copyrightOwner != ''">
            COPYRIGHT_OWNER = #{COPYRIGHT_OWNER}
        </if>
        <if test="versionNumber != null and versionNumber != ''">
            VERSION_NUMBER = #{VERSION_NUMBER}
        </if>
        <if test="recordDateStart != null and recordDateStart != ''">
            DATE_FORMAT(t.`RECORD_DATE`, '%Y-%m-%d') &gt;= DATE_FORMAT(#{recordDateStart}, '%Y-%m-%d')
        </if>
        <if test="recordDateEnd != null and recordDateEnd != '' ">
            DATE_FORMAT(t.`RECORD_DATE`, '%Y-%m-%d') &lt;= DATE_FORMAT(#{recordDateEnd}, '%Y-%m-%d')
        </if>
        <if test="developFinishDate != null and developFinishDate != ''">
            DEVELOP_FINISH_DATE = #{DEVELOP_FINISH_DATE}
        </if>
        <if test="softwareIntro != null and softwareIntro != ''">
            SOFTWARE_INTRO = #{SOFTWARE_INTRO}
        </if>
        <if test="entryPeople != null and entryPeople != ''">
            ENTRY_PEOPLE = #{ENTRY_PEOPLE}
        </if>
        <if test="entryTime != null and entryTime != ''">
            ENTRY_TIME = #{ENTRY_TIME}
        </if>
        <if test="notes != null and notes != ''">
            NOTES = #{NOTES}
        </if>
        <if test="accessoryUpload != null and accessoryUpload != ''">
            ACCESSORY_UPLOAD = #{ACCESSORY_UPLOAD}
        </if>
        and deleted != '1'
    </select>


